			function drawGraph(addr){
		var settings = {
	  "async": true,
	  "crossDomain": true,
	  "url": "http://localhost:8888/get_AdvA_details",
	  "method": "POST",
	  "headers": {
	    "adva": addr,
	    "content-type": "application/x-www-form-urlencoded",
	    "cache-control": "no-cache",
	    "postman-token": "96967447-b360-edbf-0074-e95dc54961a8"
	  },
	  "data": {
	    "AdvA": addr
	  }
	}

	$.ajax(settings).done(function (response) {
          la.push(response.channel_index_pie[0]._id);
          la.push(response.adv_type[0]._id);
          la.push(response.addr_type[0]._id);
          
          da.push(response.channel_index_pie[0].count);
          da.push(response.adv_type[0].count);
          da.push(response.addr_type[0].count);
          
      abc(la,da);
    });

    function abc(la,da){

    var ctx = document.getElementById("graph") ;

    var data = {
        labels: la,
        datasets: [{
            label: '# of Votes',
            data: da,
            backgroundColor: [
                'rgba(255, 99, 132, 0.2)',
                'rgba(54, 162, 235, 0.2)',
                'rgba(255, 206, 86, 0.2)',
                'rgba(75, 192, 192, 0.2)',
                'rgba(153, 102, 255, 0.2)',
                'rgba(255, 159, 64, 0.2)'
            ],
            borderColor: [
                'rgba(255,99,132,1)',
                'rgba(54, 162, 235, 1)',
                'rgba(255, 206, 86, 1)',
                'rgba(75, 192, 192, 1)',
                'rgba(153, 102, 255, 1)',
                'rgba(255, 159, 64, 1)'
            ],
            borderWidth: 1
        }]
    };

    var charts = new Chart(ctx, {
    type: 'pie',
    data: data,
    });
  }
}



